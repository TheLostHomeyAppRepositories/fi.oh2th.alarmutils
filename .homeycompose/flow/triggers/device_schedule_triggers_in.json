{
  "title": {
    "en": "Scheduled time is in N minutes."
  },
  "titleFormatted": {
    "en": "Scheduled time is in [[minutes]] minutes."
  },
  "hint": {
    "en": "Scheduled time is in 1-60 minutes."
  },
  "args": [
    {
      "type": "device",
      "name": "device",
      "filter": "driver_id=scheduler"
    },
    {
      "type": "number",
      "name": "minutes",
      "title": {
        "en": "minutes"
      },
      "min": 1,
      "max": 60,
      "placeholder": {
        "en": "5"
      }
    }
  ],
  "tokens": [
    {
      "type": "string",
      "name": "name",
      "title": {
        "en": "Schedule Name"
      },
      "example": {
        "en": "Schedule's name"
      }
    },
    {
      "type": "string",
      "name": "time",
      "title": {
        "en": "Schedule Time"
      },
      "example": {
        "en": "18:07"
      }
    },
    {
      "type": "string",
      "name": "next",
      "title": {
        "en": "Next Scheduled Time"
      },
      "example": {
        "en": "2023-01-23T12:09:00.000+0200"
      }
    }
  ]
}